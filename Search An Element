#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,g,found=0;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    scanf("%d",&g);
    for(int i=0;i<n;i++){
    if(a[i]==g){
    
        printf("%d is presented in an array.",g );
        found =1;
        break;

    }
    }
    if(!found){
        printf("%d is not presented in an array.",g);
    }

    return 0;
}
