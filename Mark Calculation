#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char a[20];
    int b,c,d,e,f;
    scanf("%s",a);
    scanf("%d",&b);
    scanf("%d",&c);
    scanf("%d",&d);
    scanf("%d",&e);
    scanf("%d",&f);
    printf("Name of the Student:%s",a);
    printf("\nTotal marks:%d",b+c+d+e+f);
    printf("\nAverage marks:%.1f",(float)(b+c+d+e+f)/5);

    return 0;
}
