#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int x1,x2,y1,y2;
    float x,y;
    scanf("%d",&x1);
    scanf("%d",&x2);
    scanf("%d",&y1);
    scanf("%d",&y2);
     x=(x1+y1)/2;
     y=(x2+y2)/2;
    printf("Binoy's house is located at (%.1f,%.1f)",x,y);
    return 0;
}
