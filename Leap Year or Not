#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n;
    scanf("%d",&n);
    if((n%4==0) &&(n%100))
    {
        printf("The Given Year %d is a Leap Year.",n);
    }
    else
    {
        printf("The Given Year %d is Not a Leap Year.",n);
    }
    return 0;
}
