#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a,b;
    scanf("%d",&a);
    scanf("%d",&b);
    printf("The First value before Swapping:%d",a);
    printf("\nThe Second value before Swapping:%d",b);
    printf("\nThe First value after Swapping:%d",b);
    printf("\nThe Second value after Swapping:%d",a);
    return 0;
}
